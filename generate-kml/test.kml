<?xml version="1.0" encoding="utf-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
	<Document>
		<name>Data+BalloonStyle</name>
		<visibility>1</visibility><!-- boolean -->
		<open>0</open><!-- boolean -->
		<!--
			Build Styles here, different ones for different types of placemark
		-->
		<Style id="style-id">
			<!--
				Build the Balloon style here. Text in $[name] will pull from the Placemark's <name> node.
			-->
			<BalloonStyle>
				<bgColor></bgColor><!-- kml:color -->
				<textColor></textColor><!-- kml:color -->
				<text>
					<![CDATA[

					]]>
				</text>
				<displayMode></displayMode><!-- kml:displayModeEnum, default to show balloon, hide to hide the balloon permanently -->
			</BalloonStyle>
			<!--
				Build the Icon style here. Use a specific Icon for this Style.
			-->
			<IconStyle>
				<color></color><!-- kml:color -->
				<colorMode></colorMode><!-- kml:colorModeEnum:normal or random -->
				<scale></scale><!-- float -->
				<Icon>
					<href></href><!-- http:// url to icon image -->
				</Icon>
			</IconStyle>
		</Style>
		<!--
			Repeat for each marker
		-->
		<Placemark id="longstanton">
			<name>Longstanton</name><!-- string -->
			<visibility>1</visibility><!-- boolean -->
			<open>0</open><!-- boolean -->
			<address>...</address>                <!-- string -->
			<description>...</description>        <!-- string -->
			<styleUrl>...</styleUrl>              <!-- anyURI -->
			<StyleSelector>...</StyleSelector>
			<Point id="ID">
			  <coordinates>...</coordinates><!-- lon,lat[,alt] -->
			</Point>
		</Placemark>
		<Placemark id="oakington">
			<name>Oakington</name><!-- string -->
			<visibility>1</visibility><!-- boolean -->
			<open>0</open><!-- boolean -->
			<address>...</address>                <!-- string -->
			<description>...</description>        <!-- string -->
			<styleUrl>...</styleUrl>              <!-- anyURI -->
			<StyleSelector>...</StyleSelector>
			<Point id="ID">
			  <coordinates>...</coordinates><!-- lon,lat[,alt] -->
			</Point>
		</Placemark>
		<Placemark id="girton">
			<name>Girton</name><!-- string -->
			<visibility>1</visibility><!-- boolean -->
			<open>0</open><!-- boolean -->
			<address>...</address>                <!-- string -->
			<description>...</description>        <!-- string -->
			<styleUrl>...</styleUrl>              <!-- anyURI -->
			<StyleSelector>...</StyleSelector>
			<Point id="ID">
			  <coordinates>...</coordinates><!-- lon,lat[,alt] -->
			</Point>
		</Placemark>
	</Document>
</kml>
